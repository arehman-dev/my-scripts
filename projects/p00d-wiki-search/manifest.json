{
    "name": "wiki search",
    "version": "1.0",
    "description": "Search wikipedia in popup.",
    "manifest_version": 2,
    "icons": {
        "16": "/icons/icon16",
        "48": "/icons/icon48",
        "128": "/icons/icon128"
    },
    "background": {
        "script": ["eventPage.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "contextMenus"
    ]
}