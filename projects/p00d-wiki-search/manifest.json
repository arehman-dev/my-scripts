{
    "name": "wiki search",
    "version": "1.0",
    "description": "Search wikipedia in popup.",
    "manifest_version": 2,
    "icons": {
        "16": "/icons/icon16.png",
        "48": "/icons/icon48.png",
        "128": "/icons/icon128.png"
    },
    "background": {
        "scripts": ["eventpage.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "contextMenus"
    ]
}