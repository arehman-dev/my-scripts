{
    "name": "budget_tracker",
    "version": "1.0",
    "description": "an extension that track spendings",
    "manifest_version":2,
    "icons": {
        "16": "/icons/icon16.png",
        "48": "/icons/icon48.png",
        "128": "/icons/icon128.png"
    },
    "browser_action": {
        "default_icon": "/icons/icon48.png",
        "default_popup": "/popup.html"
    },
    "permissions": [
        "storage"
    ]
}